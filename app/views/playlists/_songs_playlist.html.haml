- @recommendations['tracks'].each do |e|
  %tr
    %td
      %img{:src => "#{e['album']['images'].last['url']}", :alt => 'rss feed'}
    %td= e['name']
    %td= e['artists'].first['name']
    %td= e['album']['name']
    %td= e['popularity']
    %td
      - if current_user && current_user.spotify_refresh_token?
        = tag 'input', { value: '+', type: 'button', onclick: "add_music('#{e['id']}')", class: 'btn btn-primary green-btn', id: "#{e['id']}"}
